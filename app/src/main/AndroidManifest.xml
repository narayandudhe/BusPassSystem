<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nd.buspasssystem">

    <application
        android:allowBackup="true"
        android:icon="@drawable/home"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PassDisplay"></activity>
        <activity
            android:name=".Student"
            android:label="Student Page"></activity>
        <activity
            android:name=".Admin"
            android:label="Admin Home" />
        <activity
            android:name=".MakePass"
            android:label="Make Pass" />
        <activity
            android:name=".GetPass"
            android:label="Get Pass" />
        <activity
            android:name=".DisplayAll"
            android:label="Display All Records" />
        <activity
            android:name=".Register"
            android:label="Register" />
        <activity
            android:name=".Login"
            android:label="Bus Pass">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Main Page" />
    </application>

</manifest>